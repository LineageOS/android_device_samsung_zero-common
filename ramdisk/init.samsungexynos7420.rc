import init.samsungexynos7420.usb.rc
import init.wifi.rc
import init.baseband.rc
import init.power.rc
import init.gps.rc

on early-init
    mount debugfs /sys/kernel/debug /sys/kernel/debug mode=755

on init
    export LD_SHIM_LIBS "/system/lib64/hw/fingerprint.vendor.exynos5.so|/system/lib64/libbauthtzcommon_shim.so"

    start watchdogd

################################################
# See storage config details at http://source.android.com/tech/storage/
    mkdir /mnt/shell/emulated 0700 shell shell
    mkdir /storage/emulated 0555 root root

    mkdir /preload 0771 system system

################################################
# Export paths of external storages
    export EXTERNAL_STORAGE /sdcard
    # export SECONDARY_STORAGE /storage/extSdCard

################################################
# Support legacy paths
    symlink /sdcard /mnt/sdcard

    symlink /data/app /factory

    symlink /dev/block/platform/15570000.ufs/by-name/STEADY /dev/block/steady
    symlink /dev/block/platform/15570000.ufs/by-name/PERSISTENT /dev/block/persistent

on post-fs
################################################
# sda block device
    write /sys/block/sda/queue/scheduler "bfq"
    write /sys/block/sda/queue/read_ahead_kb 3072
    write /sys/block/sda/queue/rq_affinity 2

################################################
# sdb block device
    write /sys/block/sdb/queue/scheduler "bfq"
    write /sys/block/sdb/queue/read_ahead_kb 3072
    write /sys/block/sdb/queue/rq_affinity 2

################################################
# sdc block device
    write /sys/block/sdc/queue/scheduler "bfq"
    write /sys/block/sdc/queue/read_ahead_kb 3072
    write /sys/block/sdc/queue/rq_affinity 2

on post-fs-data
    setprop vold.post_fs_data_done 1
    mkdir /data/media 0770 media_rw media_rw

################################################
