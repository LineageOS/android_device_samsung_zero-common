    chown root system /sys/devices/virtual/fingerprint/fingerprint/disable_wake_lock
    chown root system /sys/module/workqueue/parameters/power_efficient

################################################
# Kernel tweaks
    write /proc/sys/net/ipv4/tcp_congestion_control "westwood"
    write /proc/sys/net/ipv6/conf/all/optimistic_dad 1
    write /proc/sys/net/ipv6/conf/all/use_optimistic 1
